{
  "name": "then-ref",
  "type": "module",
  "version": "0.1.1",
  "description": "",
  "author": "KID-joker <kid_joker@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/KID-joker/then-ref#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KID-joker/then-ref.git"
  },
  "bugs": "https://github.com/KID-joker/then-ref/issues",
  "keywords": ["promise", "proxy", "then", "sync", "async"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --fix",
    "start": "esno index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@types/node": "^20.12.8",
    "eslint": "^9.3.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,md}": [
      "eslint --fix"
    ]
  }
}
